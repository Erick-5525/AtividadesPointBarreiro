//7: Aumento de preço
let precoAntigo = Number(prompt("Digite o preço antigo:"));
let precoNovo;
if (precoAntigo <= 50) {
 precoNovo = precoAntigo * 1.05;
} else if (precoAntigo <= 100) {
 precoNovo = precoAntigo * 1.10;
} else {
 precoNovo = precoAntigo * 1.15;
}
console.log("Preço antigo: R$", precoAntigo.toFixed(2));
console.log("Preço novo: R$", precoNovo.toFixed(2));
